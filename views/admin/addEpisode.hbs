
    <div id="wrapper">
      {{>navbarAdmin}}

        <div id="page-wrapper">
            <div class="space"></div>
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            ADD EPISODE
                        </div>
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-12">
                                    {{#each post}}
                                    <form role="form" method="POST">
                                        <div class="form-group">
                                            <label for="disabledSelect">Name of Anime: </label>
                                            <input class="form-control" id="disabledInput" type="text" placeholder="{{this.name}}" disabled>
                                        </div>
                                        <div class="form-group">
                                            <label>Chap:</label>
                                            <input name="chap" class="form-control" placeholder="Enter Chap video... ">
                                        </div>
                                        <div class="form-group">
                                            <label>Input url Video:</label>
                                            <input name="url_video" class="form-control" placeholder="Enter Url video... ">
                                        </div>
                                        <button type="submit" class="btn btn-default">Post</button>
                                    </form>
                                    {{/each}}
                                </div>
                            </div>
                            <!-- /.row (nested) -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                </div>
                <!-- /.col-lg-12 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->
    </div>
    {{>footer}}